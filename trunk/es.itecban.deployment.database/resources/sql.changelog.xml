<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog 
xmlns="http://www.liquibase.org/xml/ns/dbchangelog/1.8"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
 xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog/1.8 http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-1.8.xsd"
 logicalFilePath="/temp/${sql.type}/sql">
 	
    <preConditions>
        <dbms type="${dbms.type}" />
    </preConditions>
    
    <changeSet author="${author.name}" id="${id.change}">
    
	        <sqlFile path="temp/${sql.type}/sql/${sql.name}"/>
        
    </changeSet>
	
	</databaseChangeLog>